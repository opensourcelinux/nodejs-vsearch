                    format: function (output) {
                        return output.replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");
                    },
                    suffix: function (input) {
                        return parseInt($(input).val()) == parseInt($(input).attr('max')) ? this.config.maxSymbol : '';
                    }
                }
            });
        }

